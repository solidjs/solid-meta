{
  "name": "solid-meta",
  "description": "Write meta tags to the document head",
  "version": "0.27.5",
  "author": "Ryan Carniato",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/solidjs/solid-meta"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "solid": "./dist/index.jsx",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
      
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist/",
    "build": "tsc && babel src/index.tsx --out-file dist/index.js && babel src/index.tsx --out-file dist/index.js --plugins @babel/plugin-transform-modules-commonjs",
    "test": "jest && npm run test:types",
    "test:types": "tsc --project tsconfig.test.json"
  },
  "peerDependencies": {
    "solid-js": ">=1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "7.14.3",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-typescript": "7.13.0",
    "@types/jest": "^26.0.23",
    "babel-preset-solid": "^1.0.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "solid-jest": "0.1.1",
    "solid-js": "^1.4.2",
    "typescript": "4.3.2"
  },
  "jest": {
    "preset": "solid-jest/preset/browser"
  }
}
